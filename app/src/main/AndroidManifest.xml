<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dg.bin.com.banner_viewpager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.OrdinaryViewPagerActivity" />
        <activity android:name=".activity.CirculationViewPagerActivity" />
        <activity android:name=".activity.AutomaticCirculationViewPagerActivity" />
        <activity android:name=".activity.OnePageMoreViewViewPagerActivity"></activity>
    </application>

</manifest>